{
	"plugins": [ "git", "ssh", "z", "g", "aliases" ],
	"prompt": [
		[ "cyan", "", "$pwd" ],
		[ "", "", " $git_lbracket$git_branch", "" ],
		[ "red", "", " $git_local_state" ],
		[ "red", "", " $git_remote_state" ],
		[ "", "", "$git_rbracket", ""],
		[ "green", "", ">" ]
	],
	"git": {
		"prompt_lbracket": "[",
		"prompt_rbracket": "]",
		"prompt_unstaged": "*",
		"prompt_staged": "+",
		"prompt_stash": "$",
		"prompt_untracked": "%",
		"prompt_remote_push": ">",
		"prompt_remote_pull": "<",
		"prompt_remote_same": "="
	}
}